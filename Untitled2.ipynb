   "source": [
    "import numpy as np\n",
    "\n",
    "def add_roots_of_unity(num_additional_roots):\n",
    "    n = num_additional_roots\n",
    "    # Generate the n-th roots of unity\n",
    "    roots_of_unity = [np.exp(2j * np.pi * k / n) for k in range(1, n + 1)]\n",
    "    return roots_of_unity"
   ]
  },
